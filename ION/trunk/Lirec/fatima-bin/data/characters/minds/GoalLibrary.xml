<GoalLibrary>

	<!-- Interest Goals -->
	
	<ActivePursuitGoal name="Goal1()">
		<PreConditions>
			<Property name="[SELF](isTestMind)" operator="=" value="True" />
			<RecentEvent occurred="False" subject="[SELF]" action="TestAction" target="test" />
		</PreConditions>
		<SucessConditions>
			<RecentEvent occurred="True" subject="[SELF]" action="TestAction" target="test" />
		</SucessConditions>
		<FailureConditions>
		</FailureConditions>
	</ActivePursuitGoal>
	
	<ActivePursuitGoal name="Goal2([anotherAgent])">
		<PreConditions>
			<Property name="[SELF](isTestMind)" operator="=" value="True" />
			<Property name="[anotherAgent](test)" operator="=" value="True" />
			<RecentEvent occurred="False" subject="[SELF]" action="TestAction2" />
		</PreConditions>
		<SucessConditions>
			<RecentEvent occurred="True" subject="[SELF]" action="TestAction2" />
		</SucessConditions>
		<FailureConditions>
		</FailureConditions>
	</ActivePursuitGoal>
	
</GoalLibrary>
